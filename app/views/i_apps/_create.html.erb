  <fieldset class="appspersonal">
<legend>Personal Information</legend>

  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :middle_name %>
    <%= f.text_field :middle_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :social_security_number %>
    <%= f.text_field :social_security_number %>
  </div>
  <div class="field dob">
    <%= f.label :date_of_birth %>
    <%= f.date_select :date_of_birth %>
  </div>
  <div class="field dln">
    <%= f.label :drivers_license_number %>
    <%= f.text_field :drivers_license_number %>
  </div>
  <div class="field">
    <%= f.label :drivers_license_state %>
    <%= f.text_field :drivers_license_state %>
  </div>
  <div class="field">
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
</fieldset>
  
  <fieldset class="appspets">
  <legend>Pets</legend>
    <%= f.fields_for :pets_attributes do |u| %>
    <%= render :partial => 'pets/create',  :locals => {:f => u }  %>
  <% end %>
</fieldset>

