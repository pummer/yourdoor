   <%= f.hidden_field :user_id %>
  <fieldset class="appssection">
<h4>Personal Information</h4>

  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name, :class => "span4"  %>
  </div>
  <div class="field">
    <%= f.label :middle_name %>
    <%= f.text_field :middle_name, :class => "span3"  %>
  </div>
  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name, :class => "span4"  %>
  </div><div class="field dln">
    <%= f.label :drivers_license_number %>
    <%= f.text_field :drivers_license_number, :class => "span2" %>
  </div>
  <div class="field">
    <%= f.label :drivers_license_state %>
    <%= f.text_field :drivers_license_state, :class => "span2" %>
  </div>
  <div class="field">
    <%= f.label :social_security_number %>
    <%= f.text_field :social_security_number, :class => "span2", :placeholder => "XXX-XX-XXXX" %>
  </div>
  <div class="field">
    <%= f.label :phone %>
    <%= f.text_field :phone, :class=> "span2", :placeholder => "XXX-XXX-XXXX" %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email, :class => "span4"   %>
  </div>

  <div class="field dob">
    <%= f.label :date_of_birth %>
    <%= f.date_select :date_of_birth, :start_year => 1900, :end_year => Time.now.year %>
  </div>


</fieldset>
  

<fieldset class="appsinside">
  <h4>Employment History</h4>
  <%= f.fields_for :employments do |u| %>
    <%= render :partial => 'employments/create',  :locals => {:f => u }  %>
  <% end %>
</fieldset>


